          d="M268.7 20.3752C269.008 20.5765 269.008 21.0274 268.7 21.2287L249.105 34.0178C248.766 34.2389 248.316 33.9951 248.316 33.59L248.316 8.01385C248.316 7.6088 248.766 7.36493 249.105 7.58612L268.7 20.3752Z"
          stroke="currentColor"
          stroke-width="2.03858"></path>
      </svg>
      <h1 class="text-[72px] font-semibold leading-[1]">{title}</h1>
      <p class="max-w-3xl text-center text-base">{description}</p>

      <div class="mt-5 flex flex-col items-center justify-center gap-6">
        <p class="font-jetBrainsMono text-sm font-light">
          Choose Conference Year
        </p>
        <div class="flex">
          {
            years &&
              years.map((cy) => (
                <a
                  href={`/akash-accelerate/${cy.year}`}
                  class={classNames(
                    "rounded-full    px-10 py-3 text-sm font-semibold",
                    {
                      "border  bg-lightGray ": cy.year === Number(year),
                      "text-para": cy.year !== Number(year),
                    }
                  )}
                >
                  {cy.year}
                </a>
              ))
          }
        </div>
      </div>
    </section>
    <section
      class="flex flex-col items-center justify-center gap-6 border-b px-10 py-10"
    >
      <p class="font-jetBrainsMono text-sm">Watch the Livestream</p>
      <div class="aspect-video w-full overflow-hidden rounded-lg">
        <iframe
          src={currentYear.live}
          class="h-full w-full"
          title="Akash Accelerate Livestream"></iframe>
      </div>
    </section>
    <section
      class="flex flex-col items-center justify-center gap-6 border-b px-10 py-10"
    >
      <p class="font-jetBrainsMono text-sm">
        Highlights from Akash Accelerate 2024
      </p>
      <div class="grid grid-cols-5 gap-5">
        {
          new Array(15)
            .fill(0)
            .map((_, index) => (
              <div class="h-10 w-full overflow-hidden rounded-lg bg-red-500" />
            ))
        }
      </div>
    </section>
  </div>
</Layout>
