---
import Layout from "@/layouts/layout.astro";
import { AccYear } from "@/utils/acc-years-data";
import classNames from "classnames";
const title = "Akash Accelerate";
const description =
  "Akash Accelerate is a full-day summit that brings together the entire ecosystem for a packed agenda of talks, panels, and discussions exploring the future of decentralized compute and open-source AI.";

type Props = {
  year: string;
  years: AccYear[];
  currentYear: AccYear;
};
const { year, years, currentYear } = Astro.props;
---

<Layout
  title={`${title} - ${year}`}
  description={description}
  image="/meta-images/acc-2024.png"
>
  <div class="flex flex-col py-10">
    <section
      class="flex flex-col items-center justify-center gap-5 border-b pb-6"
    >
      <svg
        width="296"
        height="42"
        viewBox="0 0 296 42"
        fill="none"
        xmlns="http://www.w3.org/2000/svg"
