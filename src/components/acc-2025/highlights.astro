---
import { acc2025 } from "./acc-2025";
---

<a
  href={acc2025.youtube}
  target="_blank"
  class="group relative flex aspect-[16/6] max-h-[640px] w-full"
>
  <img
    src="/images/acc-2025/sky.png"
    alt="Akash Accelerate 2025 Highlights"
    class="relative z-[1] h-full w-full object-cover"
  />
  <div class="container-nav-2 absolute inset-0 z-[2] flex flex-col gap-2 py-10">
    <svg
      width="85"
      height="85"
      viewBox="0 0 85 85"
      fill="none"
      xmlns="http://www.w3.org/2000/svg"
    >
      <circle cx="42.5" cy="42.5002" r="42.5" fill="#21B35D"></circle>
      <path d="M61 43.5002L30.25 60.3877L30.25 26.6127L61 43.5002Z" fill="white"
      ></path>
    </svg>
    <h2
      class="text-5xl font-semibold leading-[1.2] transition-all group-hover:underline"
    >
      View Highlights <br />
      From Last Year
    </h2>
  </div>
</a>
